  <div class="container" id="header-box">
    <h1>Challenge #{{challenge.id}}: {{challenge.name}}</h1>
    <p>{{challenge.instructionsAll}}</p>
  </div>
  <div class="container">
    <div class="container columns" id="your-code">
        <h2>Your Task</h2>
        <p>{{challenge.instructions}}</p>
    </div>
    <div class="container columns" id="partner-code">
        <h2>{{challenge.partnerDisplayName}}'s Task</h2>
        <p>{{challenge.partnerInstructions}}</p>
    </div>
  </div>
  <div class="container bump-up">
    <div class="container columns" id="your-code">
      <div id="code-mirror-container"></div>
        <input type="hidden" id="myTestArgument" value="{{challenge.testArgument}}">
        <input type="hidden" id="myTestResult" value="{{challenge.testResult}}">
        <br clear="all">
        <button class="testMyCode">Test My Code</button>
    </div>
    <div class="container columns" id="partner-code">
        <div class="code-input">
          <!-- partner's code will come from sockets -->
        </div>
    </div>
  </div>
  <div class="container">
    <h2>Chat</h2>
    <input type="text" name="chatbox" size="50" id="chatbox"><button id="submit-chat">Send</button>
    <p id="chat-history">

    </p>
  </div>
  <script type="text/javascript">
    //get data from server
    var sessionData = {
        sessionId : {{session.id}},
        challengeId : {{challenge.id}}
    };
  </script>
  <script src="/js/codemirror.js"></script>
  <link rel="stylesheet" href="/css/codemirror.css">
  <script src="/js/codemirror-mode.js"></script>
  <script src="/js/challenge.js" type="text/javascript"></script>
  <script src="/js/chat.js" type="text/javascript"></script>
  <div class="dataHolder" data-displayname="{{user.displayName}}" data-userid="{{user.id}}" data-matchId="{{session.id}}" data-partnername="{{challenge.partnerDisplayName}}" data-startCode="{{challenge.startCode}}"></div>
